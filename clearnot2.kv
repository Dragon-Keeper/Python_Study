RootWidget:
    # import container
    container: container

    # fill the container
    BoxLayout:
        id: container
        orientation: 'horizontal'
        padding: 250  # decide Label's Y_position
        spacing: 300
        canvas:
            Color:
                rgba: 0, 0, 0, root.clearnot_color
